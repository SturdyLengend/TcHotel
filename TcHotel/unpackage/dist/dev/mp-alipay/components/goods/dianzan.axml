<view class="goods_list"><view data-event-opts="{{[['tap',[['itemchange',['$0'],[[['msg','',index]]]]]]]}}" class="goods_item" onTap="__e" a:for="{{msg}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tianchong"><image src="{{item.defaultImage}}" mode="scaleToFill"></image></view><view class="{{((isend==true)?'xuanfu':'')}}"><text class="fu">{{item.storeName}}</text></view><view class="price"><text class="tex">{{item.evaluation}}</text></view><view class="bao"><view class="tupian"><image src="{{item.avatarLink}}"></image></view><view class="info"><text>{{item.nickname}}</text></view><view data-event-opts="{{[['tap',[['ai',['$0'],['istrue']]]]]}}" class="{{(('item')+' '+((istrue==true)?'active':''))}}" catchTap="__e">‚ù§</view></view></view></view>