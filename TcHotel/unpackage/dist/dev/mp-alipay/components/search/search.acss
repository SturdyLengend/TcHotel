.searchall {
  margin-top: 40rpx;
  width: 100%;
  margin-left: 25rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.searchall swiper {
  width: 95%;
  height: 60rpx;
}
.searchall swiper swiper-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.searchall swiper swiper-item text {
  display: block;
  background-color: #ccc;
  width: 200rpx;
  height: 60rpx;
  text-align: center;
  border: 10rpx solid #fff;
  opacity: 0.7;
}

