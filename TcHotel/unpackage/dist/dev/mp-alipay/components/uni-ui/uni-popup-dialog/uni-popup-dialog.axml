<view class="uni-popup-dialog data-v-a2c82ea8"><view class="uni-dialog-title data-v-a2c82ea8"><text class="{{(('uni-dialog-title-text data-v-a2c82ea8')+' '+('uni-popup__'+dialogType))}}">{{title}}</text></view><view class="uni-dialog-content data-v-a2c82ea8"><block a:if="{{mode==='base'}}"><text class="uni-dialog-content-text data-v-a2c82ea8">{{content}}</text></block><block a:else><input class="uni-dialog-input data-v-a2c82ea8" type="text" placeholder="{{placeholder}}" focus="{{focus}}" data-event-opts="{{[['input',[['__set_model',['','val','$event',[]]]]]]}}" value="{{val}}" onInput="__e"/></block></view><view class="uni-dialog-button-group data-v-a2c82ea8"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="uni-dialog-button data-v-a2c82ea8" onTap="__e"><text class="uni-dialog-button-text data-v-a2c82ea8">取消</text></view><view data-event-opts="{{[['tap',[['onOk',['$event']]]]]}}" class="uni-dialog-button uni-border-left data-v-a2c82ea8" onTap="__e"><text class="uni-dialog-button-text uni-button-color data-v-a2c82ea8">确定</text></view></view></view>